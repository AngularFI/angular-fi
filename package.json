{
  "name": "angular.fi",
  "version": "1.1.3",
  "description": "Angular Finland website",
  "homepage": "http://www.angular.fi/",
  "repository": "https://github.com/AngularFI/angular.fi",
  "author": {
    "name": "Jussi Kinnula",
    "email": "jussi.kinnula@gmail.com"
  },
  "contributors": [
    {
      "name": "Toni Ristola",
      "email": "toni.ristola@gofore.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.0.0"
  },
  "scripts": {
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config config/webpack.production.ts --progress",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.development.ts --progress",
    "build:watch": "webpack --config config/webpack.development.ts --watch",
    "build": "npm run build:prod",
    "clean": "rimraf target",
    "predev:frontend": "npm run clean",
    "dev:frontend": "npm run server:dev:frontend",
    "predev:backend": "npm run clean && npm run build:dev",
    "dev:backend": "concurrently \"npm run server:dev:backend\" \"npm run build:watch\"",
    "dev": "npm run dev:frontend",
    "postinstall": "npm run build",
    "server:prod": "node target/server.js",
    "server:dev:frontend": "webpack-dev-server --config config/webpack.devserver.ts",
    "server:dev:backend": "nodemon --exec 'ts-node' ./src/server.ts",
    "server:dev": "npm run server:dev:frontend",
    "server": "npm run server:prod",
    "start": "npm run server"
  },
  "dependencies": {
    "@angular/common": "^4.3.0",
    "@angular/compiler": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/forms": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@angular/platform-browser-dynamic": "^4.3.0",
    "@angular/router": "^4.3.0",
    "@types/body-parser": "^1.16.4",
    "@types/express": "^4.0.36",
    "@types/hammerjs": "^2.0.34",
    "@types/jasmine": "^2.5.53",
    "@types/mime": "^1.3.1",
    "@types/mongodb": "^2.2.7",
    "@types/mongoose": "^4.7.18",
    "@types/node": "^8.0.13",
    "@types/redis": "^2.6.0",
    "@types/request": "0.0.47",
    "@types/serve-static": "^1.7.31",
    "angular2-masonry": "jussikinnula/angular2-masonry#build",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^7.1.2",
    "body-parser": "^1.17.2",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "email-validator": "^1.0.7",
    "express": "^4.15.3",
    "favicons-webpack-plugin": "0.0.7",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.29.0",
    "http-auth": "^3.2.3",
    "js.clone": "^0.0.3",
    "moment": "^2.18.1",
    "mongoose": "^4.11.3",
    "postcss-loader": "^2.0.6",
    "pug": "^2.0.0-rc.2",
    "pug-html-loader": "^1.1.5",
    "raw-loader": "^0.5.1",
    "redis": "^2.7.1",
    "request": "^2.81.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.4.2",
    "serve-static": "^1.12.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^2.3.0",
    "ts-node": "~3.2.0",
    "typescript": "^2.4.1",
    "webpack": "^3.3.0",
    "webpack-bundle-analyzer": "^2.8.3",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.13"
  },
  "devDependencies": {
    "@types/webpack": "^3.0.4",
    "concurrently": "^3.5.0",
    "dotenv-cli": "^1.4.0",
    "jasmine-core": "^2.6.4",
    "karma": "^1.7.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.4",
    "nodemon": "~1.11.0",
    "webpack-dev-server": "~2.5.1"
  }
}
