{
  "name": "angular.fi",
  "version": "1.0.1",
  "description": "Angular Finland website",
  "homepage": "http://www.angular.fi/",
  "repository": "https://github.com/AngularFI/angular.fi",
  "author": {
    "name": "Jussi Kinnula",
    "email": "jussi.kinnula@gmail.com"
  },
  "contributors": [
    {
      "name": "Toni Ristola",
      "email": "toni.ristola@gofore.com"
    }
  ],
  "license": "MIT",
  "engines": {
    "node": ">=7",
    "npm": ">=4"
  },
  "scripts": {
    "prebuild:prod": "npm run clean",
    "build:prod": "webpack --config config/webpack.production.ts --progress",
    "prebuild:dev": "npm run clean",
    "build:dev": "webpack --config config/webpack.development.ts --progress",
    "build": "npm run build:prod",
    "clean": "rimraf target",
    "predev:frontend": "npm run clean",
    "dev:frontend": "npm run server:dev:frontend",
    "predev:backend": "npm run clean && npm run build:dev",
    "dev:backend": "concurrently \"npm run server:dev:backend\" \"npm run watch\"",
    "dev": "npm run dev:frontend",
    "postinstall": "npm run build",
    "server:prod": "node target/server/index.js",
    "server:dev:frontend": "webpack-dev-server --config config/webpack.devserver.ts",
    "server:dev:backend": "nodemon target/server/index.js",
    "server:dev": "npm run server:dev:frontend",
    "server": "npm run server:prod",
    "start": "npm run server",
    "watch": "webpack --config config/webpack.development.ts --watch"
  },
  "dependencies": {
    "@angular/common": "^4.0.1",
    "@angular/compiler": "^4.0.1",
    "@angular/core": "^4.0.1",
    "@angular/forms": "^4.0.1",
    "@angular/http": "^4.0.1",
    "@angular/platform-browser": "^4.0.1",
    "@angular/platform-browser-dynamic": "^4.0.1",
    "@angular/router": "^4.0.1",
    "@types/express": "^4.0.35",
    "@types/hammerjs": "^2.0.34",
    "@types/mime": "^0.0.29",
    "@types/mongodb": "^2.1.42",
    "@types/mongoose": "^4.7.9",
    "@types/node": "^7.0.12",
    "@types/redis": "^0.12.36",
    "@types/request": "0.0.42",
    "@types/serve-static": "^1.7.31",
    "angular2-masonry": "jussikinnula/angular2-masonry#testing",
    "angular2-router-loader": "^0.3.5",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.7",
    "compression": "^1.6.2",
    "copy-webpack-plugin": "^4.0.1",
    "core-js": "^2.4.1",
    "del": "^2.2.2",
    "del-cli": "^0.2.1",
    "express": "^4.15.2",
    "favicons-webpack-plugin": "0.0.7",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "http-auth": "^3.1.3",
    "js.clone": "^0.0.3",
    "moment": "^2.18.1",
    "mongoose": "^4.9.3",
    "postcss-loader": "^1.3.3",
    "pug": "^2.0.0-beta6",
    "pug-html-loader": "^1.1.4",
    "raw-loader": "^0.5.1",
    "redis": "^2.7.1",
    "request": "^2.81.0",
    "rimraf": "^2.6.1",
    "rxjs": "^5.3.0",
    "serve-static": "^1.12.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "ts-helpers": "^1.1.2",
    "ts-loader": "^2.0.3",
    "ts-node": "~3.0.2",
    "typescript": "^2.2.2",
    "webpack": "^2.3.3",
    "webpack-bundle-analyzer": "^2.3.1",
    "webpack-merge": "^4.1.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "concurrently": "~3.4.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-jasmine": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "nodemon": "~1.11.0",
    "webpack-dev-server": "~2.4.2"
  }
}
